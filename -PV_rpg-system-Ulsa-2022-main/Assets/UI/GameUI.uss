.partyContainer {
    align-items: stretch;
    background-color: rgba(117, 186, 250, 0.14);
    position: relative;
    bottom: 0;
    width: 90%;
    flex-grow: 1;
    flex-direction: row;
    justify-content: center;
    border-top-left-radius: 15px;
    border-bottom-left-radius: 15px;
    border-top-right-radius: 15px;
    border-bottom-right-radius: 15px;
    border-left-width: 2px;
    border-right-width: 2px;
    border-top-width: 2px;
    border-bottom-width: 2px;
    border-left-color: rgb(255, 255, 255);
    border-right-color: rgb(255, 255, 255);
    border-top-color: rgb(255, 255, 255);
    border-bottom-color: rgb(255, 255, 255);
}

.title {
    background-color: rgba(0, 0, 0, 0);
    color: rgb(255, 255, 255);
    font-size: 24px;
}

.profile {
    background-image: url('project://database/Assets/ExampleAssets/Textures/Props/Construction_Light/ConstructionLight_MetallicOcculusionSmoothness.tif?fileID=2800000&guid=9a81f1a59f171804e84fce76584bdcb7&type=3#ConstructionLight_MetallicOcculusionSmoothness');
    height: 100px;
    width: 100px;
}

.characterInfo {
    margin-top: 20px;
    margin-bottom: 20px;
    margin-left: 50px;
    margin-right: 50px;
}

.container {
    position: absolute;
    width: 100%;
    align-items: center;
    bottom: 20px;
}

.health {
    background-color: rgb(0, 0, 0);
}

